    <em:id>coscripter@almaden.ibm.com</em:id>

    <!-- nsIUpdateItem type for a Multiple-Item Package -->
    <em:type NC:parseType="Integer">32</em:type>
    <em:version>2.2.7</em:version>
    <em:name>CoScripter</em:name>
    <em:description>Capture, share, and automate business processes</em:description>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>99.0.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>

</RDF>
