<?xml version="1.0"?>
<RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#"
  xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <Description about="urn:mozilla:install-manifest">
    <em:name>CoScripter Platform</em:name>
    <em:description>Base Library for Supporting CoScripter Functionality</em:description>
    <em:id>platform@coscripter.ibm.com</em:id>
    <em:creator>CoScripter Team</em:creator>
    <em:contributor>Allen Cypher</em:contributor>
    <em:contributor>Clemens Drews</em:contributor>
    <em:contributor>Eser Kandogan</em:contributor>
    <em:contributor>Tessa Lau</em:contributor>
    <em:contributor>James Lin</em:contributor>
    <em:contributor>Greg Little</em:contributor>
    <em:contributor>Jeffrey Nichols</em:contributor>
    <em:homepageURL>http://coscripter.researchlabs.ibm.com/</em:homepageURL>
    <em:version>2.2.7</em:version>
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <!-- toolkit@mozilla.org -->
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9.0.11</em:minVersion>
        <em:maxVersion>99.0.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:requires>
      <Description>
        <em:id>yule@coscripter.ibm.com</em:id>
        <em:minVersion>2.0.0</em:minVersion>
        <em:maxVersion>2.*</em:maxVersion>
      </Description>
    </em:requires>
  </Description>
</RDF>
